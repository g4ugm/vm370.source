     TITLE 'DMSNXM: Display the NUCEXT control block chain'             00010000
**************************************************************          00020000
* Simulate the NUCXMAP command in later CMS levels.          *          00030000
**************************************************************          00040000
DMSNXM   CSECT                                                          00050000
         USING NUCON,0                                                  00060000
         STM   R14,R12,12(R13)                                          00070000
         LR    R12,R15                                                  00080000
         USING DMSNXM,R12                                               00090000
*                                                              HRC424DS 00090100
         MVC   EXTNAME,=CL8'*'    Assume all extensions wanted HRC431DS 00090110
         MVC   SEGNAME,=CL8' '    Assume no SEGMENT name       HRC431DS 00090120
         MVI   OPTFLAG,0          Clear option bits            HRC424DS 00090200
         MVI   OPTFLAG2,0         Clear control bits           HRC431DS 00090210
         LA    R3,8(,R1)          Point to name in Plist       HRC424DS 00090300
         CLI   0(R3),X'FF'        Present?                     HRC424DS 00090400
         BE    OPTSET             No, Name is missing          HRC424DS 00090500
         CLI   0(R3),C'('                                      HRC424DS 00090600
         BE    HAVEOPT            Skip down if options start   HRC424DS 00090700
         MVC   EXTNAME,0(R3)      Wants specific extension     HRC431DS 00090810
         LA    R3,8(,R3)          Skip to next parameter       HRC424DS 00090900
*                                                              HRC424DS 00091000
         CLI   0(R3),X'FF'        Any options?                 HRC424DS 00091100
         BE    OPTSET             No, that's OK                HRC424DS 00091200
         CLI   0(R3),C'('                                      HRC424DS 00091300
         BE    HAVEOPT            Process options if so        HRC424DS 00091400
         B     INVOPT             Invalid argument             HRC424DS 00091500
*                                                              HRC424DS 00091600
HAVEOPT  DS    0H                                              HRC424DS 00091700
         LA    R3,8(,R3)          Skip to 1st Option name      HRC424DS 00091800
         CLI   0(R3),X'FF'        Any options?                 HRC424DS 00091900
         BE    OPTSET             No, that's OK                HRC424DS 00092000
OPTLKUP  DS    0H                                              HRC424DS 00092100
         BAL   R14,OPTOPLEN       Get option length - 1 in R15 HRC424DS 00092200
         LA    R2,OPTTBL          Start of option table        HRC424DS 00092300
         LA    R4,OPTTBLNE        Number of entries            HRC424DS 00092400
OPTLKUP2 DS    0H                                              HRC424DS 00092500
         CLM   R15,B'0001',8(R2)  Check against entry min size HRC424DS 00092600
         BL    OPTLKUP3           Provided name too short      HRC424DS 00092700
         CLC   0(*-*,R2),0(R3)                                 HRC424DS 00092800
         EX    R15,*-6            Does this option name match? HRC424DS 00092900
         BNE   OPTLKUP3           If, not, go check next       HRC424DS 00093000
*                                                              HRC424DS 00093100
         OC    OPTFLAG,9(R2)      Set option bit               HRC424DS 00093200
*                                                              HRC424DS 00093300
*                                 Check for default direction  HRC424DS 00093400
         TM    9(R2),OPTSTACK     Was it STACK?                HRC424DS 00093500
         BZ    OPTNOSTK           Skip if not                  HRC424DS 00093600
         LA    R3,8(,R3)          Step to next token           HRC424DS 00093700
         CLI   0(R3),X'FF'        Any more options?            HRC424DS 00093800
         BE    OPSTKLST           Loop done if not             HRC431DS 00093910
         BAL   R14,OPTOPLEN       Get option length - 1 in R15 HRC424DS 00094000
         CLC   0(*-*,R3),CHKLIFO                               HRC424DS 00094100
         EX    R15,*-6            Is it LIFO?                  HRC424DS 00094200
         BE    OPTLKUP            Go to top of loop to set it  HRC424DS 00094300
         CLC   0(*-*,R3),CHKFIFO                               HRC424DS 00094400
         EX    R15,*-6            Is it FIFO?                  HRC424DS 00094500
         BE    OPTLKUP            Go to top of loop to set it  HRC424DS 00094600
         OI    OPTFLAG,OPTFIFO    Default direction is FIFO    HRC424DS 00094700
         B     OPTLKUP            Go check the others          HRC424DS 00094800
OPSTKLST DS    0H                 Here if last option is STACK HRC431DS 00094810
         OI    OPTFLAG,OPTFIFO    Default direction is FIFO             00094820
         B     OPTSET             Loop done                    HRC431DS 00094830
OPTNOSTK DS    0H                                              HRC424DS 00094900
*                                 Check for default direction  HRC424DS 00095000
         TM    9(R2),OPTSEGM      Was it SEGMENT?              HRC424DS 00095100
         BZ    OPTNOSGN           Skip if not                  HRC424DS 00095200
         LA    R3,8(,R3)          Step to next token           HRC424DS 00095300
         CLI   0(R3),X'FF'        Any more options?            HRC424DS 00095400
         BE    MISSSEG            No, operand missing          HRC424DS 00095500
         MVC   SEGONAME,0(R3)     Save SEGMENT operand here    HRC424DS 00095600
OPTNOSGN DS    0H                                              HRC424DS 00095700
*                                                              HRC424DS 00095800
         LA    R3,8(,R3)          Step to next token           HRC424DS 00095900
         CLI   0(R3),X'FF'        Any more options?            HRC424DS 00096000
         BNE   OPTLKUP            Go process if so             HRC424DS 00096100
         B     OPTSET             Continue below               HRC424DS 00096200
*                                                              HRC424DS 00096300
OPTLKUP3 DS    0H                                              HRC424DS 00096400
         LA    R2,OPTENTLN(,R2)   Skip to next tbl entry       HRC424DS 00096500
         BCT   R4,OPTLKUP2          and go check that one      HRC424DS 00096600
         B     INVOPT             No match on supplied option  HRC424DS 00096700
*                                                              HRC424DS 00096800
OPTOPLEN DS    0H                 Get option length - 1 in R15 HRC424DS 00096900
         LA    R15,7(,R3)         Point to byte 8 of option    HRC424DS 00097000
         CLI   0(R15),C' '        Is is blank                  HRC424DS 00097100
         BNE   *+8                Yes skip out                 HRC424DS 00097200
         BCT   R15,*-8            Go test previous byte        HRC424DS 00097300
*                                                              HRC424DS 00097400
         LA    R15,1(,R15)                                     HRC424DS 00097500
         SR    R15,R3             User supplied option length  HRC424DS 00097600
         BCTR  R15,0                 minus one                 HRC424DS 00097700
         BR    R14                                             HRC424DS 00097800
*                                                              HRC424DS 00097900
OPTSET   DS    0H                 All option bits are set      HRC424DS 00098000
*                                                              HRC424DS 00098100
*                                                                       00100000
         L     R2,NUCXCBLK        List anchor                           00110000
         LTR   R2,R2              Anything there?                       00120000
         BNZ   CHKNAME            Go check for name match      HRC431DS 00131000
         WRTERM 'No Nucleus Extensions are loaded'                      00132000
         SR    R15,R15                                         HRC431DS 00133000
         B     GOBACK             Done                                  00160000
*                                                              HRC431DS 00170100
*                                 Get entry to display         HRC431DS 00170200
CHKNAME  DS    0H                                              HRC431DS 00170300
         USING SCBLOCK,R2                                      HRC431DS 00170400
         CLC   EXTNAME,=CL8'*'    Skip to DISPLAY label        HRC431DS 00170500
         BE    DISPLAY              if all extensions          HRC431DS 00170600
         CLC   EXTNAME,=CL8'ALL'       are being selected      HRC431DS 00170700
         BE    DISPLAY                                         HRC431DS 00170800
*                                 Here to search by name       HRC431DS 00170900
CHKNAME2 DS    0H                                              HRC431DS 00171000
         CLC   EXTNAME,SCBNAME    Found the right name entry?  HRC431DS 00171100
         BE    DISPLAY                                         HRC431DS 00171200
         ICM   R2,B'1111',SCBFWPTR  Skip to the next SCB       HRC431DS 00171300
         BNZ   CHKNAME2                                        HRC431DS 00171400
         LINEDIT TEXT='Nucleus extension name is not loaded'   HRC431DS 00171500
         SR    R15,R15                                         HRC431DS 00171600
         B     GOBACK             Done                         HRC431DS 00171700
*                                                              HRC431DS 00171800
DISPLAY  DS    0H                                              HRC431DS 00171900
*                                 Get length of output line    HRC431DS 00172000
         LA    R7,LINELEN3                                     HRC431DS 00172100
         TM    OPTFLAG,OPTATTR+OPTSEGI attributes+seginfo?     HRC431DS 00172200
         BO    MAINLOOP           Yes, longest line            HRC431DS 00172300
         LA    R7,LINELEN2                                     HRC431DS 00172400
         TM    OPTFLAG,OPTSEGI    seginfo w/o attributes       HRC431DS 00172500
         BO    MAINLOOP           Yes, shortest line           HRC431DS 00172600
         LA    R7,LINELEN         attributes w/o seginfo       HRC431DS 00172700
         OI    OPTFLAG,OPTATTR    Default is show attributes   HRC431DS 00172800
*                                                              HRC431DS 00172900
*                                                                       00200000
         USING SCBLOCK,R2                                               00210000
MAINLOOP DS    0H                                                       00220000
*  Apply display filter options                                HRC424DS 00220100
         TM    OPTFLAG,OPTSEGM    SEGMENT selection option?    HRC424DS 00220200
         BZ    DISPLAY2           No, display all              HRC424DS 00220300
         TM    SCBSFLG2,SCBSFSEG  Segment nucleus extension?   HRC424DS 00220400
         BZ    NEXTSCB            If not, skip the display     HRC424DS 00220500
*                                                              HRC424DS 00220600
         CLC   SEGONAME,=CL8'*'   Show all segments?           HRC424DS 00220700
         BE    DISPLAY2           Display it if so             HRC424DS 00220800
         CLC   SEGONAME,SCBSEGID  Does the name match?         HRC424DS 00220900
         BE    DISPLAY2           Display it if so             HRC424DS 00221000
         B     NEXTSCB            If not, skip the display     HRC424DS 00221100
DISPLAY2 DS    0H                                              HRC424DS 00221200
         MVI   LINE,C' '          Blank out the line                    00230000
         LR    R15,R7             Length of output line        HRC431DS 00240100
         BCTR  R15,0                - 1                        HRC431DS 00240200
         MVC   LINE+1(*-*),LINE   Pre-blank the output line    HRC431Ds 00240300
         EX    R15,*-6                                         HRC431DS 00240400
         MVC   NAME,SCBNAME                                             00250000
*                                                                       00260000
         L     R3,SCBENTR         Entry address in binary               00270000
         ST    R3,TEMPSAVE               Binary                         00280000
         UNPK  TEMPSAVE+8(9),TEMPSAVE(5) Unpack                         00290000
         TR    TEMPSAVE+8(8),DMPTAB-C'0' Trans to hex chars             00300000
         MVC   ENTRY(8),TEMPSAVE+8 Copy to output line                  00310000
         L     R3,SCBWKWRD        Work Word in binary                   00330000
         ST    R3,TEMPSAVE               Binary                         00340000
         UNPK  TEMPSAVE+8(9),TEMPSAVE(5) Unpack                         00350000
         TR    TEMPSAVE+8(8),DMPTAB-C'0' Trans to hex chars             00360000
         MVC   USERWORD(8),TEMPSAVE+8 Copy to output line               00370000
*                                                                       00380000
         L     R3,SCBXORG         Code address in binary                00390000
         ST    R3,TEMPSAVE               Binary                         00400000
         UNPK  TEMPSAVE+8(9),TEMPSAVE(5) Unpack                         00410000
         TR    TEMPSAVE+8(8),DMPTAB-C'0' Trans to hex chars             00420000
         MVC   ORIGIN(8),TEMPSAVE+8 Copy to output line                 00430000
*                                                                       00440000
         L     R3,SCBXLEN         Code length in binary                 00450000
         ST    R3,TEMPSAVE               Binary                         00460000
         UNPK  TEMPSAVE+8(9),TEMPSAVE(5) Unpack                         00470000
