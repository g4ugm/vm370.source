         STC   R0,12(R15)         Set line length              HRC431DS 00800435
         STCM  R1,B'0111',13(R15) Set line address             HRC431DS 00800440
         MVC   16(8,R15),=8X'FF'  Set fence                    HRC431DS 00800445
         LR    R1,R15                                          HRC431DS 00800450
         SVC   202                                             HRC431DS 00800455
         DC    AL4(1)                                          HRC431DS 00800460
         BR    R14                                             HRC424DS 00800500
SHOWWRT  DS    0H                                              HRC424DS 00800600
         TM    OPTFLAG2,OPTHEADR  Has title line been sent?    HRC431DS 00800710
         BO    SHOWLINE           Display input line if so     HRC424DS 00800800
         LR    R4,R0                                           HRC424DS 00800900
