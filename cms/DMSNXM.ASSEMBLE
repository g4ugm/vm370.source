         MVC   BYTES(8),TEMPSAVE+8  Copy to output line                 00490000
*                                                                       00500000
         MVC   AMODE(2),=C'24'                                          00510000
*                                                              HRC431DS 00510100
         SR    R14,R14            Assume no SEGINFO            HRC431DS 00510200
         TM    OPTFLAG,OPTSEGI    SEGINFO option?              HRC431DS 00510300
         BZ    SKIPSEGI           Use other data if so         HRC431DS 00510400
         LA    R14,8+1            Offset for attributes        HRC431DS 00510500
         TM    SCBSFLG2,SCBSFSEG  Segment nucleus extension?   HRC431DS 00510600
         BZ    SKIPSEGI           If not, skip the display     HRC431DS 00510700
         MVC   LSEGNAME,SCBSEGID  Set segment name             HRC431DS 00510800
SKIPSEGI DS    0H                                              HRC431DS 00510900
         TM    OPTFLAG,OPTATTR    Attributes?                  HRC431DS 00511000
         BZ    SHOWLIN2           Skip next if note            HRC431DS 00511100
*                                                                       00520000
         TM    SCBSFLAG,SCBSFSYS  SYSTEM flag set?                      00530000
         BZ    *+14               Skip next if not             HRC431DS 00550010
         LA    R15,SYSTEM(R14)    Where SYSTEM goes            HRC431DS 00550020
         MVC   0(6,R15),=C'SYSTEM'  Set it                     HRC431DS 00550030
*                                                                       00560000
         TM    SCBSFLAG,SCBSFSER  SERVICE flag set?                     00570000
         BZ    *+14               Skip next if not             HRC431DS 00580100
         LA    R15,SERVICE(R14)   Where SERVICE goes           HRC431DS 00580200
         MVC   0(7,R15),=C'SERVICE' Set it                     HRC431DS 00580300
*                                                                       00600000
         TM    SCBSFLAG,SCBSFEND  ENDCMD flag set?                      00610000
         BZ    *+14               Skip next if not             HRC431DS 00620100
         LA    R15,ENDCMD(R14)    Where ENDCMD goes            HRC431DS 00620200
         MVC   0(6,R15),=C'ENDCMD'  Set it                     HRC431DS 00620300
*                                                                       00640000
*        TM    SCBSFLAG,SCBSFSER  IMMCMD flag set?    Maybe later       00650000
         BZ    *+14               Skip next if not             HRC431DS 00660100
         LA    R15,IMMCMD(R14)    Where IMMCMD goes            HRC431DS 00660200
         MVC   0(6,R15),=C'IMMCMD'  Set it (maybe later)       HRC431DS 00660300
*                                                                       00680000
         TM    SCBSFLAG,SCBSPERM  PERM flag set?                        00690000
         BZ    *+14               Skip next if not             HRC431DS 00700100
         LA    R15,PERM(R14)      Where PERM goes              HRC431DS 00700200
         MVC   0(4,R15),=C'PERM'  Set it                       HRC431DS 00700300
*                                                                       00720000
SHOWLIN2 DS    0H                                              HRC431DS 00720010
         LA    R1,LINE            Display report line          HRC424DS 00730100
         LR    R0,R7              Line length                  HRC431DS 00730210
         BAL   R14,SHOWMSG                                     HRC424DS 00730300
NEXTSCB  DS    0H                 On to next SCBLOCK           HRC424DS 00731000
         ICM   R2,B'1111',SCBFWPTR                                      00740000
         BNZ   MAINLOOP                                                 00750000
         SR    R15,R15                                                  00760000
         B     GOBACK             Done                         HRC424DS 00760100
*                                                              HRC424DS 00760200
MISSSEG  DS    0H                                              HRC424DS 00760300
         LINEDIT TEXT='SEGMENT option name is missing',        HRC424DS*00760400
               SUB=(CHARA,((R3),8))                            HRC424DS 00760500
         LA    R15,24             Not found RC                 HRC424DS 00760600
         B     GOBACK             Done                         HRC424DS 00760700
*                                                              HRC424DS 00760800
INVOPT   DS    0H                                              HRC424DS 00760900
         LINEDIT TEXT='Invalid option: ........',              HRC424DS*00761000
               SUB=(CHARA,((R3),8))                            HRC424DS 00761100
         LA    R15,24             Not found RC                 HRC424DS 00761200
         B     GOBACK             Done                         HRC424DS 00761300
*                                                              HRC424DS 00761400
GOBACK   DS    0H                                                       00770000
         L     R14,12(,R13)                                             00780000
         LM    R0,R12,20(R13)                                           00790000
         BR    R14                                                      00800000
SHOWMSG  DS    0H                                              HRC424DS 00800100
         TM    OPTFLAG,OPTFIFO+OPTLIFO Stack or display        HRC424DS 00800200
         BZ    SHOWWRT            Display on terminal if not   HRC424DS 00800300
*        stack the line here                                   HRC424DS 00800400
         LA    R15,ATTNPLST                                    HRC431DS 00800405
         MVI   8(R15),C'L'        Last in first out            HRC431DS 00800410
         TM    OPTFLAG,OPTLIFO    Was it LIFO?                 HRC431DS 00800415
         BO    *+8                Bif so                       HRC431DS 00800420
         MVI   8(R15),C'F'        First in first out           HRC431DS 00800425
